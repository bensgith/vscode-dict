{
  "name": "vscode-dictionary",
  "displayName": "VS Dictionary",
  "description": "An English dictionary for VS Code",
  "publisher": "BingxiLiang",
  "icon": "./resources/dict256.png",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Education"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vsdict",
          "title": "VS Dictionary",
          "icon": "./resources/dictionary128.svg"
        }
      ]
    },
    "views": {
      "vsdict": [
        {
          "type": "webview",
          "id": "dictionary.search",
          "name": "Search"
        },
        {
          "type": "tree",
          "id": "dictionary.history",
          "name": "History"
        }
      ]
    },
    "commands": [
      {
        "command": "dictionary.clearHistory",
        "title": "Clear History",
        "icon": "$(clear-all)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "dictionary.clearHistory",
          "when": "view == dictionary.history",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "node ./esbuild.js",
    "package": "node ./esbuild.js",
    "watch": "node ./esbuild.js --watch",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/node": "^20.10.1",
    "@types/vscode": "^1.84.2",
    "@types/vscode-webview": "^1.57.4",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "esbuild": "^0.19.8",
    "esbuild-plugin-copy": "^2.1.1",
    "eslint": "^8.54.0",
    "glob": "^10.3.10",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^1.3.1",
    "axios": "^1.6.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bensgith/vscode-dict.git"
  },
  "license": "SEE LICENSE IN LICENSE.txt"
}