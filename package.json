{
  "name": "vscode-dict",
  "displayName": "vscode-dict",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Education"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "dict-search",
          "title": "Dictionary",
          "icon": "$(book)"
        }
      ]
    },
    "views": {
      "dict-search": [
        {
          "type": "webview",
          "id": "dictionary.view",
          "name": ""
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "node ./esbuild.js",
    "package": "node ./esbuild.js",
    "watch": "node ./esbuild.js --watch",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.9.4",
    "@types/vscode": "^1.84.2",
    "@types/vscode-webview": "^1.57.4",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vscode/test-electron": "^2.3.7",
    "esbuild": "^0.19.7",
    "esbuild-plugin-copy": "^2.1.1",
    "eslint": "^8.54.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^1.3.1",
    "axios": "^1.6.2"
  }
}
